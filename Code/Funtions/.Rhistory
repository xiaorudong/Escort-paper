install.packages("devtools")
devtools::install_github("hemberg-lab/SC3")
library(cluster)
library("readxl")
my_data <- read_excel("/Volumes/bachergroup/TN19_Paper/DATA/TN19\ Clinical\ Data_20181221.xlsx", sheet = "MMTT")
my_data <- my_data[3:nrow(my_data),]
colnames(my_data) <- my_data[1,]
my_data <- my_data[-1,c(1:4, 16)]
subdata <- subset(my_data, Visit %in% c("Screening", "12 weeks", "24 weeks", "36 weeks", "48 weeks"))
subdata$Days <- rep(NA, nrow(subdata))
subdata$Days[subdata$Visit=="Screening"] <- 1
subdata$Days[subdata$Visit=="12 weeks"] <- 12*7
subdata$Days[subdata$Visit=="24 weeks"] <- 24*7
subdata$Days[subdata$Visit=="36 weeks"] <- 36*7
subdata$Days[subdata$Visit=="48 weeks"] <- 48*7
subdata$Days[subdata$Visit=="Screening"] <- 0
subdata$Days[subdata$Visit=="12 weeks"] <- 12
subdata$Days[subdata$Visit=="24 weeks"] <- 24
subdata$Days[subdata$Visit=="36 weeks"] <- 36
subdata$Days[subdata$Visit=="48 weeks"] <- 48
subdata$`2hr AUC` <- as.numeric(subdata$`2hr AUC`)
library(lme4)
library(nlme)
mod = lmer(log(`2hr AUC`+1) ~ Days + ((Days-1) | `Masked ID`)+ (1 | `Masked ID`) , data = subdata)
summary(mod)
est_slope <- ranef(mod)$`Masked ID`
est_slope$id <- rownames(est_slope)
orig_slope <- read.csv("/Volumes/bachergroup/TN19_Paper/DATA/cpeptide_slopes_from_models.TN19_cpeptide_slopes_2019-01-16.csv")
all_slope <- merge(est_slope, orig_slope[,1:2], by.x="id", by.y="masked_id")
plot(all_slope$Days, all_slope$slope_random_effect)
cor(all_slope$Days, all_slope$slope_random_effect)
summary(mod)
est_slope
View(subdata)
